  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#95a5a6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings, Constants",
      "scope": [
        "string",
        "constant"
      ],
      "settings": {
        "foreground": "#e84393"
      }
    },
    {
      "name": "Keywords, Storage Types",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#8c7ae6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes, Types, Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "support.class"
      ],
      "settings": {
        "foreground": "#00a8a8",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#00a8a8"
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#2c3e50"
      }
    },
    {
      "name": "Object Keys, Property Names",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#00a8a8"
      }
    },
    {
      "name": "Variable Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#2c3e50",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation - General",
      "scope": "punctuation",
      "settings": {
        "foreground": "#95a5a6"
      }
    },
    {
      "name": "Punctuation - Accessors",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator.dot",
        "punctuation.separator.namespace"
      ],
      "settings": {
        "foreground": "#2c3e50"
      }
    },
    {
      "name": "Punctuation - Structural",
      "scope": [
        "punctuation.section.block",
        "punctuation.section.embedded",
        "punctuation.section.class",
        "punctuation.section.method",
        "punctuation.definition.parameters",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7f8c8d"
      }
    },
    {
      "name": "HTML/XML/JSX Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#8c7ae6"
      }
    },
    {
      "name": "HTML/XML/JSX Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#00a8a8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#7f8c8d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Keywords/Tags",
      "scope": [
        "keyword.other.documentation"
      ],
      "settings": {
        "foreground": "#95a5a6",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Invalid or Deprecated",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#c0392b",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": "heading.markup.md",
      "settings": {
        "foreground": "#8c7ae6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.md",
      "settings": {
        "foreground": "#2c3e50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic.md",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#00a8a8",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code Blocks",
      "scope": [
        "markup.fenced_code_block.md",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#e84393"
      }
    },
    {
      "name": "Jupyter Magic Commands",
      "scope": "keyword.control.ipython",
      "settings": {
        "foreground": "#c5a5ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Jupyter Notebook Comments",
      "scope": "comment.line.number-sign.python",
      "settings": {
        "foreground": "#00a8a8",
        "fontStyle": "italic"
      }
    }
  ]
}