    "gitDecoration.ignoredResourceForeground": "#8a8899"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5a528a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#e84393"
      }
    },
    {
      "name": "Numbers, Booleans, Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#e84393"
      }
    },
    {
      "name": "Keywords, Storage Types",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a29bfe",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes, Types, Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "support.class"
      ],
      "settings": {
        "foreground": "#81ecec",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#81ecec"
      }
    },
    {
      "name": "Variables and Parameters",
      "scope": [
        "variable",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#dcd7ff"
      }
    },
    {
      "name": "Object Keys, Property Names",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#81ecec"
      }
    },
    {
      "name": "Variable Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#dcd7ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#5a528a"
      }
    },
    {
      "name": "HTML/XML/JSX Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#a29bfe"
      }
    },
    {
      "name": "HTML/XML/JSX Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#81ecec",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation Accessors",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator.dot",
        "punctuation.separator.namespace"
      ],
      "settings": {
        "foreground": "#dcd7ff"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#8a8899",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Keywords/Tags",
      "scope": [
        "keyword.other.documentation"
      ],
      "settings": {
        "foreground": "#5a528a",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Invalid or Deprecated",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ff6b81",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Structural Punctuation",
      "scope": [
        "punctuation.section.block",
        "punctuation.section.embedded",
        "punctuation.section.class",
        "punctuation.section.method",
        "punctuation.definition.parameters",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#8a8899"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": "heading.markup.md",
      "settings": {
        "foreground": "#a29bfe",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.md",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#dcd7ff"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic.md",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#81ecec",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code Blocks",
      "scope": [
        "markup.fenced_code_block.md",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#e84393"
      }
    }
  ]
}